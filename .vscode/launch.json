  "version": "0.2.0",
  "configurations": [
    {
      "name": "Seed Resources",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/api/cmd/seed_resources",
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "Seed Modules",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/api/cmd/seed_modules/main.go",
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "Seed Mappings",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/api/cmd/seed_mappings/main.go",
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "Run API",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/api/cmd/server",
      "envFile": "${workspaceFolder}/.env",
    },
  ]
}
