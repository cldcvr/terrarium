  "version": "0.2.0",
  "configurations": [
    {
      "name": "Seed Resources",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/cli/main.go",
      "args": ["farm", "resources"],
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "Seed Modules",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/cli/main.go",
      "args": ["farm", "modules"],
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "Seed Mappings",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/cli/main.go",
      "args": ["farm", "mappings"],
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
    },
    {
      "name": "Run API",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/src/api/main.go",
      "envFile": "${workspaceFolder}/.env",
    },
  ]
}
